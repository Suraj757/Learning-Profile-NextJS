{
  "name": "learning-profile-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:consolidation": "jest tests/consolidation-scenarios.test.js",
    "test:algorithms": "jest tests/consolidation-algorithms.test.js",
    "test:integration": "jest tests/integration-workflow.test.js",
    "test:all-consolidation": "jest tests/consolidation-scenarios.test.js tests/consolidation-algorithms.test.js tests/integration-workflow.test.js",
    "test:manual": "node scripts/test-consolidation-system.js --manual",
    "test:api": "node scripts/test-consolidation-system.js",
    "test:coverage": "jest --coverage",
    "test:performance": "jest src/__tests__/performance --testTimeout=60000",
    "test:load": "node src/__tests__/performance/load-testing/run-load-tests.js",
    "test:memory": "node --expose-gc src/__tests__/performance/memory-testing/memory-leak-detection.js",
    "test:benchmark": "node src/__tests__/performance/benchmarks/api-benchmarks.js",
    "test:perf-all": "npm run test:performance && npm run test:load && npm run test:memory && npm run test:benchmark",
    "perf:quick": "node src/__tests__/performance/performance-suite-runner.js quick",
    "perf:standard": "node src/__tests__/performance/performance-suite-runner.js standard",
    "perf:comprehensive": "node src/__tests__/performance/performance-suite-runner.js comprehensive",
    "perf:ci": "node src/__tests__/performance/performance-suite-runner.js ci",
    "perf:nightly": "node src/__tests__/performance/performance-suite-runner.js nightly",
    "load:quick": "node src/__tests__/performance/load-testing/run-load-tests.js quick",
    "load:standard": "node src/__tests__/performance/load-testing/run-load-tests.js standard",
    "load:comprehensive": "node src/__tests__/performance/load-testing/run-load-tests.js comprehensive",
    "load:stress": "node src/__tests__/performance/load-testing/run-load-tests.js stress",
    "memory:short": "node --expose-gc src/__tests__/performance/memory-testing/memory-leak-detection.js",
    "memory:long": "node --expose-gc src/__tests__/performance/memory-testing/memory-leak-detection.js --long",
    "benchmark:api": "node src/__tests__/performance/benchmarks/api-benchmarks.js",
    "benchmark:load": "node src/__tests__/performance/load-testing/autocannon-load-tests.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "lucide-react": "^0.539.0",
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "node-fetch": "^3.3.2",
    "supertest": "^6.3.4",
    "typescript": "^5",
    "autocannon": "^7.15.0",
    "clinic": "^13.0.0",
    "0x": "^5.7.0",
    "perf_hooks": "^0.0.1",
    "worker_threads": "^1.0.0",
    "v8": "^0.1.0"
  }
}
